<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>WetterwarnungDownloader</title>
	<parser>
		<target>tmp/cache</target>
		<extensions>
			<extension>module</extension>
			<extension>inc</extension>
			<extension>php</extension>
		</extensions>
	</parser>
	<transformer>
		<target>doc/api</target>
	</transformer>
	<files>
		<ignore>tests/*</ignore>
		<ignore>Tests/*</ignore>
		<ignore>Temp/*</ignore>
		<ignore>temp/*</ignore>
		<ignore>config.sample.php</ignore>
	</files>
</phpdoc>